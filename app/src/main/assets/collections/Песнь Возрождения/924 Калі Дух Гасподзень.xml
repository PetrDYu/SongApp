<song number="924" name="Калі Дух Гасподзень" canon="false" text="" textRus="" music="">
    <verse number="1">
        <string>
            <plain>Калі Дух Гасподзень</plain>
        </string>
        <string>
            <plain>Напауняе мяне,</plain>
        </string>
        <string>
            <plain>Спяваю, як Давід.</plain>
        </string>
        <string>
            <plain>Спяваю, Спяваю,</plain>
        </string>
        <string>
            <plain>Спяваю, як Давід.</plain>
        </string>
    </verse>
    <verse number="2">
        <string>
            <plain>Калі Дух Гасподзень</plain>
        </string>
        <string>
            <plain>Напауняе мяне,</plain>
        </string>
        <string>
            <plain>Малюся, як Давід.</plain>
        </string>
        <string>
            <plain>Малюся, Малюся,</plain>
        </string>
        <string>
            <plain>Малюся, як Давід.</plain>
        </string>
    </verse>
    <verse number="3">
        <string>
            <plain>Калі Дух Гасподзень</plain>
        </string>
        <string>
            <plain>Напауняе мяне,</plain>
        </string>
        <string>
            <plain>Я плачу, як Давід.</plain>
        </string>
        <string>
            <plain>Я плачу, Я плачу,</plain>
        </string>
        <string>
            <plain>Я плачу, як Давід.</plain>
        </string>
    </verse>
    <verse number="4">
        <string>
            <plain>Калі Дух Гасподзень</plain>
        </string>
        <string>
            <plain>Напауняе мяне,</plain>
        </string>
        <string>
