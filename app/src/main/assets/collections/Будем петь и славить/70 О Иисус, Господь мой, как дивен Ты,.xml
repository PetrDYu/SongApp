<song number="70" name="О Иисус, Господь мой, как дивен Ты," canon="false" text="" textRus="" music="" additionalInfo="">
    <verse number="0">
        <string>
            <plain>О Иисус, Господь мой, как дивен Ты,</plain>
        </string>
        <string>
            <plain>О, как Ты благ, как полон любви.</plain>
        </string>
