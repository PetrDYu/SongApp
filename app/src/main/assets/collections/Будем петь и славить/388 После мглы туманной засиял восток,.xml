<?xml version='1.0' encoding='utf-8'?>
<song number="388" name="После мглы туманной засиял восток," canon="false" text="" textRus="" music="" additionalInfo="">
    <verse number="1">
        <string>
            <plain>После мглы туманной засиял восток,</plain>
        </string>
        <string>
            <repeat id="0" rep_rate="-1" is_opening="true" />
            <plain>Льется непрестанно радости поток.</plain>
            <repeat id="0" rep_rate="2" is_opening="false" />
        </string>
    </verse>
    <verse number="2">
        <string>
            <plain>Воплотилось Слово по любви Отца.</plain>
        </string>
        <string>
            <repeat id="1" rep_rate="-1" is_opening="true" />
            <plain>Рождество Христово – радость всем сердцам.</plain>
            <repeat id="1" rep_rate="2" is_opening="false" />
        </string>
    </verse>
</song>