<?xml version='1.0' encoding='utf-8'?>
<song number="63" name="Славим, славим, всем сердцем славим мы Тебя" canon="false" text="Д. Дж. Хаддена" textRus="" music="Д. Дж. Хаддена" additionalInfo="">
    <verse number="1">
        <string>
            <plain>Славим, славим, всем сердцем славим мы Тебя.</plain>
        </string>
        <string>
            <plain>Славим, славим, мы всей душою славим.</plain>
        </string>
    </verse>
    <verse number="2">
        <string>
            <plain>Хвалим, хвалим...</plain>
        </string>
    </verse>
    <verse number="3">
        <string>
            <plain>Верим, верим...</plain>
        </string>
    </verse>
    <verse number="4">
        <string>
            <plain>Любим, любим...</plain>
        </string>
    </verse>
</song>