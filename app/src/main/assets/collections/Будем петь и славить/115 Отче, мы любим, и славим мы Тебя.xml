<song number="115" name="Отче, мы любим, и славим мы Тебя" canon="false" text="" textRus="" music="" additionalInfo="">
    <verse number="1">
        <string>
            <plain>Отче, мы любим, и славим мы Тебя</plain>
        </string>
        <string>
            <plain>Славим Твое имя на земле.</plain>
        </string>
        <string>
            <plain>Славим мы Тебя, славим мы Тебя,</plain>
        </string>
        <string>
            <plain>Славим Твое имя на земле.</plain>
        </string>
    </verse>
