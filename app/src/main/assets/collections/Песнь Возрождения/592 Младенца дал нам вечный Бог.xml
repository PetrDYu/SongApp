<song number="592" name="Младенца дал нам вечный Бог" canon="false" text="" textRus="" music="">
    <verse number="1">
        <string>
            <plain>Младенца дал нам вечный Бог;</plain>
        </string>
        <string>
            <plain>Нам Сын дан от Него;</plain>
        </string>
        <string>
            <plain>Владычества святой залог</plain>
        </string>
        <string>
            <plain>На раменах Его.</plain>
        </string>
        <string>
            <plain>Владычества святой залог</plain>
        </string>
        <string>
            <plain>На раменах Его.</plain>
        </string>
    </verse>
    <verse number="2">
        <string>
            <plain>Его Бог крепкий и Творец -</plain>
        </string>
        <string>
            <plain>Мир целый назовет</plain>
        </string>
        <string>
            <plain>Советник, вечности Отец</plain>
        </string>
        <string>
            <plain>И Чудный в род и род.</plain>
        </string>
        <string>
            <plain>Советник, вечности Отец</plain>
        </string>
        <string>
            <plain>И Чудный в род и род.</plain>
        </string>
    </verse>
    <verse number="3">
        <string>
            <plain>Владычество Он укрепит;</plain>
        </string>
        <string>
            <plain>Предела мира нет;</plain>
        </string>
        <string>
            <plain>Навек Он правдой утвердит</plain>
        </string>
        <string>
            <plain>Престол Свой и завет.</plain>
        </string>
        <string>
            <plain>Навек Он правдой утвердит</plain>
        </string>
        <string>
            <plain>Престол Свой и завет.</plain>
        </string>
    </verse>
</song>