<?xml version='1.0' encoding='utf-8'?>
<song number="1660" name="Аллилуйя Аллилуйя Буду славить я Того" canon="false" text="" textRus="" music="">
    <verse number="1">
        <string>
            <plain>Аллилуйя! Аллилуйя!</plain>
        </string>
        <string>
            <plain>Буду славить я Того,</plain>
        </string>
        <string>
            <plain>Кто всех дороже.</plain>
        </string>
        <string>
            <plain>Буду славить я Царя</plain>
        </string>
        <string>
            <plain>Здесь на земле и в небесах.</plain>
        </string>
        <string>
            <plain>Аллилуйя! Аллилуйя!</plain>
        </string>
    </verse>
    <verse number="2">
        <string>
            <plain>Аллилуйя! Аллилуйя!</plain>
        </string>
        <string>
            <plain>Он страдал за нас,</plain>
        </string>
        <string>
            <plain>кровь проливая.</plain>
        </string>
        <string>
            <plain>И за это благодарность</plain>
        </string>
        <string>
            <plain>Все Ему мы воспоем.</plain>
        </string>
        <string>
            <plain>Аллилуйя! Аллилуйя!</plain>
        </string>
    </verse>
    <verse number="3">
        <string>
            <plain>Аллилуйя! Аллилуйя!</plain>
        </string>
        <string>
            <plain>Буду петь от всей души</plain>
        </string>
        <string>
            <plain>Тебе, Спаситель.</plain>
        </string>
        <string>
            <plain>Иисус, благодарю</plain>
        </string>
        <string>
            <plain>Тебя за дар любви святой.</plain>
        </string>
        <string>
            <plain>За прощенье, за спасенье.</plain>
        </string>
    </verse>
</song>