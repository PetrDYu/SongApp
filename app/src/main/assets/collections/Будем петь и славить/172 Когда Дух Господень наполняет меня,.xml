<?xml version='1.0' encoding='utf-8'?>
<song number="172" name="Когда Дух Господень наполняет меня," canon="false" text="" textRus="" music="" additionalInfo="">
    <verse number="1">
        <string>
            <repeat id="0" rep_rate="-1" is_opening="true" />
            <plain>Когда Дух Господень наполняет меня,</plain>
        </string>
        <string>
            <plain>я верю, как Авраам.</plain>
            <repeat id="0" rep_rate="2" is_opening="false" />
        </string>
        <string>
            <repeat id="1" rep_rate="-1" is_opening="true" />
            <plain>Я верю, я верю, я верю, как Авраам.</plain>
            <repeat id="1" rep_rate="2" is_opening="false" />
        </string>
    </verse>
    <verse number="2">
        <string>
            <repeat id="2" rep_rate="-1" is_opening="true" />
            <plain>Когда Дух Господень наполняет меня,</plain>
        </string>
        <string>
            <plain>учу я, как Моисей.</plain>
            <repeat id="2" rep_rate="2" is_opening="false" />
        </string>
        <string>
            <repeat id="3" rep_rate="-1" is_opening="true" />
            <plain>Учу я, учу я, учу я, как Моисей.</plain>
            <repeat id="3" rep_rate="2" is_opening="false" />
        </string>
    </verse>
    <verse number="3">
        <string>
            <repeat id="4" rep_rate="-1" is_opening="true" />
            <plain>Когда Дух Господень наполняет меня,</plain>
        </string>
        <string>
            <plain>пою я, как царь Давид.</plain>
            <repeat id="4" rep_rate="2" is_opening="false" />
        </string>
        <string>
            <repeat id="5" rep_rate="-1" is_opening="true" />
            <plain>Пою я, пою я, пою я, как царь Давид.</plain>
            <repeat id="5" rep_rate="2" is_opening="false" />
        </string>
    </verse>
    <verse number="4">
        <string>
            <repeat id="6" rep_rate="-1" is_opening="true" />
            <plain>Когда Дух Господень наполняет меня,</plain>
        </string>
        <string>
            <plain>молюсь я, как Даниил.</plain>
            <repeat id="6" rep_rate="2" is_opening="false" />
        </string>
        <string>
            <repeat id="7" rep_rate="-1" is_opening="true" />
            <plain>Молюсь я, молюсь я, молюсь я, как Даниил.</plain>
            <repeat id="7" rep_rate="2" is_opening="false" />
        </string>
    </verse>
    <verse number="5">
        <string>
            <repeat id="8" rep_rate="-1" is_opening="true" />
            <plain>Когда Дух Господень наполняет меня,</plain>
        </string>
        <string>
            <plain>люблю я, как Иисус.</plain>
            <repeat id="8" rep_rate="2" is_opening="false" />
        </string>
        <string>
            <repeat id="9" rep_rate="-1" is_opening="true" />
            <plain>Люблю я, люблю я, люблю я, как Иисус.</plain>
            <repeat id="9" rep_rate="2" is_opening="false" />
        </string>
    </verse>
    <verse number="6">
        <string>
            <repeat id="10" rep_rate="-1" is_opening="true" />
            <plain>О Дух Господень, Дух Святой,</plain>
        </string>
        <string>
            <plain>приди и наполни нас.</plain>
            <repeat id="10" rep_rate="2" is_opening="false" />
        </string>
        <string>
            <repeat id="11" rep_rate="-1" is_opening="true" />
            <plain>Наполни, наполни, приди и наполни нас.</plain>
            <repeat id="11" rep_rate="2" is_opening="false" />
        </string>
    </verse>
</song>