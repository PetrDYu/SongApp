<song number="22" name="Пойте Богу песнь хвалы" canon="false" text="" textRus="" music="Ж. Бертье" additionalInfo="">
    <verse number="0">
        <string>
            <plain>Пойте Богу песнь хвалы,</plain>
        </string>
        <string>
            <plain>Славьте Господа всегда.</plain>
        </string>
        <string>
            <plain>Новой песней славьте Его.</plain>
        </string>
    </verse>
    <verse number="0">
        <string>
            <plain>Singt dem Herr ein neues Lied.</plain>
        </string>
        <string>
            <plain>Lobsingt Ihm allezeit,</plain>
        </string>
        <string>
            <plain>lobsingt Ihm allezeit!</plain>
        </string>
    </verse>
</song>