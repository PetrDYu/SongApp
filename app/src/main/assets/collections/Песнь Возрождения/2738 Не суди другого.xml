<?xml version='1.0' encoding='utf-8'?>
<song number="2738" name="Не суди другого" canon="false" text="" textRus="" music="">
    <verse number="1">
        <string>
            <plain>Не суди ,другого</plain>
        </string>
        <string>
            <plain>3а его грехи</plain>
        </string>
        <string>
            <plain>Избегай худого,</plain>
        </string>
        <string>
            <plain>За собой смотри.</plain>
        </string>
    </verse>
    <verse number="2">
        <string>
            <plain>Ты ведь непрестанно</plain>
        </string>
        <string>
            <plain>Худо сам творил</plain>
        </string>
        <string>
            <plain>Помолись смиренно,</plain>
        </string>
        <string>
            <plain>Чтоб Господь простил.</plain>
        </string>
    </verse>
    <verse number="3">
        <string>
            <plain>И когда опять ты</plain>
        </string>
        <string>
            <plain>Видишь, зло творят,</plain>
        </string>
        <string>
            <plain>Суд не совершай ты,</plain>
        </string>
        <string>
            <plain>А учись прощать.</plain>
        </string>
    </verse>
    <verse number="4">
        <string>
            <plain>Всех людей Он любит</plain>
        </string>
        <string>
            <plain>И хранит от зла,</plain>
        </string>
        <string>
            <plain>Кто послушен будет</plain>
        </string>
        <string>
            <plain>- Примет в небеса.</plain>
        </string>
    </verse>
</song>