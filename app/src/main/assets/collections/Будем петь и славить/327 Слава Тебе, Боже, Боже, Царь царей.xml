<?xml version='1.0' encoding='utf-8'?>
<song number="327" name="Слава Тебе, Боже, Боже, Царь царей" canon="false" text="Давида Лодена" textRus="" music="Давида Лодена" additionalInfo="">
    <verse number="0">
        <string>
            <plain>Слава Тебе, Боже, Боже, Царь царей!</plain>
        </string>
        <string>
            <repeat id="0" rep_rate="-1" is_opening="true" />
            <plain>Ты, Господь, солнце правды.</plain>
        </string>
        <string>
            <plain>Мир под крылом Твоим.</plain>
            <repeat id="0" rep_rate="2" is_opening="false" />
        </string>
    </verse>
    <verse number="0">
        <string>
            <plain>А́ДОН А́-КАВО́Д МЭ́ЛЕХ А́-МЛАХИ́М</plain>
        </string>
        <string>
            <repeat id="1" rep_rate="-1" is_opening="true" />
            <plain>ШЭ́МЭШ ЦДАКА́ УМА́РПЭ́ БИ́ХНАФЕ́ЙА́</plain>
            <repeat id="1" rep_rate="2" is_opening="false" />
        </string>
    </verse>
</song>