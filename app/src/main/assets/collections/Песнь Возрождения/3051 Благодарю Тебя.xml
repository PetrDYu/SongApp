<?xml version='1.0' encoding='utf-8'?>
<song number="3051" name="Благодарю Тебя" canon="false" text="" textRus="" music="">
    <verse number="1">
        <string>
            <plain>Благодарю Тебя,</plain>
        </string>
        <string>
            <plain>Создатель мой,</plain>
        </string>
        <string>
            <plain>что жизнь Ты спас</plain>
        </string>
        <string>
            <plain>мою, благодарю!</plain>
        </string>
        <string>
            <plain>Иду я за Тобой,</plain>
        </string>
        <string>
            <plain>Спаситель мой;</plain>
        </string>
        <string>
            <plain>Тебя благодарю,</plain>
        </string>
        <string>
            <plain>Создатель мой,</plain>
        </string>
        <string>
            <plain>Создатель мой.</plain>
        </string>
    </verse>
    <verse number="2">
        <string>
            <plain>И О любви Твоей,</plain>
        </string>
        <string>
            <plain>что дал Ты мне,</plain>
        </string>
        <string>
            <plain>я возвещать пойду</plain>
        </string>
        <string>
            <plain>о ней везде.</plain>
        </string>
        <string>
            <plain>Безмерно велика</plain>
        </string>
        <string>
            <plain>любовь Твоя,</plain>
        </string>
        <string>
            <plain>и всем она дана,</plain>
        </string>
        <string>
            <plain>любовь Твоя,</plain>
        </string>
        <string>
            <plain>любовь Твоя.</plain>
        </string>
    </verse>
    <verse number="3">
        <string>
            <plain>За жизнь, за мир</plain>
        </string>
        <string>
            <plain>Тебя благодарю,</plain>
        </string>
        <string>
            <plain>за свет и за покой</plain>
        </string>
        <string>
            <plain>благодарю!</plain>
        </string>
        <string>
            <plain>За то, что счастлив я,</plain>
        </string>
        <string>
            <plain>хвалу пою;</plain>
        </string>
        <string>
            <plain>Тебя, Создатель мой,</plain>
        </string>
        <string>
            <plain>благодарю!</plain>
        </string>
        <string>
            <plain>Благодарю.</plain>
        </string>
    </verse>
</song>