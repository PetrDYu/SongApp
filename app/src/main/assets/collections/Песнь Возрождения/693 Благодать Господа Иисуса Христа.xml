<?xml version='1.0' encoding='utf-8'?>
<song number="693" name="Благодать Господа Иисуса Христа" canon="false" text="" textRus="" music="">
    <verse number="1">
        <string>
            <plain>Благодать Господа Иисуса Христа,</plain>
        </string>
        <string>
            <plain>И любовь Бога Отца,</plain>
        </string>
        <string>
            <plain>И общение Святого Духа</plain>
        </string>
        <string>
            <plain>Да будет с нами.</plain>
        </string>
        <string>
            <plain>Аминь.</plain>
        </string>
    </verse>
</song>