<?xml version='1.0' encoding='utf-8'?>
<song number="2081" name="Господу господсгвующих слава" canon="false" text="" textRus="" music="">
    <verse number="1">
        <string>
            <plain>Господу господсгвующих слава!</plain>
        </string>
        <string>
            <plain>Царю царей слава, слава, слава!</plain>
        </string>
        <string>
            <plain>Царю царей, Царю царей</plain>
        </string>
        <string>
            <plain>Слава, слава, слава!</plain>
        </string>
    </verse>
    <verse number="2">
        <string>
            <plain>Буду превозносить Тебя,</plain>
        </string>
        <string>
            <plain>Боже мой, Царь мой,</plain>
        </string>
        <string>
            <plain>И благословлять имя</plain>
        </string>
        <string>
            <plain>Твоё Во веки и веки.</plain>
        </string>
    </verse>
    <verse number="3">
        <string>
            <plain>Господу господствуютцих слава!</plain>
        </string>
        <string>
            <plain>Царю царей,</plain>
        </string>
        <string>
            <plain>Царю царей слава, слава, слава!</plain>
        </string>
        <string>
            <plain>Царю царей,</plain>
        </string>
        <string>
            <plain>Царю царей Слава, слава, слава!</plain>
        </string>
    </verse>
    <verse number="4">
        <string>
            <plain>Уста мои изрекут хвалу Господню,</plain>
        </string>
        <string>
            <plain>И да благословляет</plain>
        </string>
        <string>
            <plain>всякая плоть святое имя Его</plain>
        </string>
        <string>
            <plain>Во веки и веки.</plain>
        </string>
    </verse>
    <verse number="5">
        <string>
            <plain>Господу господствуютцих слава!</plain>
        </string>
        <string>
            <plain>Царю царей,</plain>
        </string>
        <string>
            <plain>Царю царей слава, слава, слава!</plain>
        </string>
        <string>
            <plain>Царю царей,</plain>
        </string>
        <string>
            <plain>Царю царей Слава, слава, слава!</plain>
        </string>
    </verse>
</song>