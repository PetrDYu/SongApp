<?xml version='1.0' encoding='utf-8'?>
<song number="92" name="Господь, Ты сотворил мир" canon="false" text="и муз Э. Беренджер" textRus="" music="" additionalInfo="">
    <verse number="1">
        <string>
            <repeat id="0" rep_rate="-1" is_opening="true" />
            <plain>Господь, Ты сотворил мир,</plain>
        </string>
        <string>
            <plain>Аллилуйя, аллилуйя!</plain>
            <repeat id="0" rep_rate="2" is_opening="false" />
        </string>
    </verse>
    <chorus number="1">
        <string>
            <repeat id="1" rep_rate="-1" is_opening="true" />
            <plain>Славен Бог, славен Бог</plain>
        </string>
        <string>
            <plain>Во веки веков!</plain>
            <repeat id="1" rep_rate="2" is_opening="false" />
        </string>
    </chorus>
    <verse number="2">
        <string>
            <repeat id="2" rep_rate="-1" is_opening="true" />
            <plain>Господь, Ты сотворил нас,</plain>
        </string>
        <string>
            <plain>Аллилуйя, аллилуйя!</plain>
            <repeat id="2" rep_rate="2" is_opening="false" />
        </string>
    </verse>
    <verse number="3">
        <string>
            <repeat id="3" rep_rate="-1" is_opening="true" />
            <plain>Господь от смерти нас спас,</plain>
        </string>
        <string>
            <plain>Аллилуйя, аллилуйя!</plain>
            <repeat id="3" rep_rate="2" is_opening="false" />
        </string>
    </verse>
    <verse number="4">
        <string>
            <repeat id="4" rep_rate="-1" is_opening="true" />
            <plain>Господь, Ты Духа нам дал,</plain>
        </string>
        <string>
            <plain>Аллилуйя, аллилуйя!</plain>
            <repeat id="4" rep_rate="2" is_opening="false" />
        </string>
    </verse>
    <verse number="5">
        <string>
            <repeat id="5" rep_rate="-1" is_opening="true" />
            <plain>Господь, Ты отдал нам мир,</plain>
        </string>
        <string>
            <plain>Аллилуйя, аллилуйя!</plain>
            <repeat id="5" rep_rate="2" is_opening="false" />
        </string>
    </verse>
    <verse number="6">
        <string>
            <repeat id="6" rep_rate="-1" is_opening="true" />
            <plain>Господь, Ты вечность нам дал,</plain>
        </string>
        <string>
            <plain>Аллилуйя, аллилуйя!</plain>
            <repeat id="6" rep_rate="2" is_opening="false" />
        </string>
    </verse>
</song>