<?xml version='1.0' encoding='utf-8'?>
<song number="2594" name="Пришел в мир Спаситель" canon="false" text="" textRus="" music="">
    <verse number="1">
        <string>
            <plain>Пришел в мир Спаситель,</plain>
        </string>
        <string>
            <plain>Дал людям свободу.</plain>
        </string>
        <string>
            <plain>Он опять придет</plain>
        </string>
        <string>
            <plain>Взять от земли своих,</plain>
        </string>
        <string>
            <plain>А где будешь ты?</plain>
        </string>
    </verse>
    <verse number="2">
        <string>
            <plain>Тебя возлюбил Он</plain>
        </string>
        <string>
            <plain>Отцовской любовью</plain>
        </string>
        <string>
            <plain>Поспеши скорей</plain>
        </string>
        <string>
            <plain>Приди к Нему и ты,</plain>
        </string>
        <string>
            <plain>Спеши, не опоздай!</plain>
        </string>
    </verse>
    <verse number="3">
        <string>
            <plain>И небо, и море,</plain>
        </string>
        <string>
            <plain>И звезды, и горы,</plain>
        </string>
        <string>
            <plain>Говорит нам всё:</plain>
        </string>
        <string>
            <plain>Скоро Христос придет,</plain>
        </string>
        <string>
            <plain>Спеши, не опоздай!</plain>
        </string>
    </verse>
    <verse number="4">
        <string>
            <plain>Господь милосердный</plain>
        </string>
        <string>
            <plain>Зовет тебя снова,</plain>
        </string>
        <string>
            <plain>Так не медли, друг,</plain>
        </string>
        <string>
            <plain>Приди к нему скорей,</plain>
        </string>
        <string>
            <plain>Спеши, не опоздай!</plain>
        </string>
        <string>
            <plain>Не опоздай, не опаздай!</plain>
        </string>
    </verse>
</song>